<Window x:Class="Tehtava1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tehtava1"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <CheckBox x:Name="checkBox" Content="Milk" HorizontalAlignment="Left" Margin="83,19,0,0" VerticalAlignment="Top"/>
        <CheckBox x:Name="checkBox1" Content="Butter" HorizontalAlignment="Left" Margin="83,50,0,0" VerticalAlignment="Top"/>
        <CheckBox x:Name="checkBox2" Content="Beer" HorizontalAlignment="Left" Margin="83,86,0,0" VerticalAlignment="Top"/>
        <CheckBox x:Name="checkBox3" Content="Chicken" HorizontalAlignment="Left" Margin="83,122,0,0" VerticalAlignment="Top"/>
        <CheckBox x:Name="checkBox4" Content="Lemonade" HorizontalAlignment="Left" Margin="83,155,0,0" VerticalAlignment="Top"/>
        <Button x:Name="button" Content="Button" HorizontalAlignment="Left" Margin="83,191,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>
        <TextBlock x:Name="textBlock" HorizontalAlignment="Left" Margin="83,230,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="204" Height="58"/>

    </Grid>
</Window>
